{
  "name": "citibike-api",
  "version": "1.0.0",
  "description": "Simple backend that sends citibike trip data",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "serve": "nodemon index.js --exec babel-node",
    "build": "npm run clean && babel lib -d dist",
    "start": "node dist/index.js",
    "heroku-postbuild": "node_modules/osrm/lib/binding/osrm-extract ./nyc_data/nyc_data.osm.pbf -p node_modules/osrm/profiles/bicycle.lua
    && node_modules/osrm/lib/binding/osrm-contract new_york_data/new-york_new-york.osrm"
  },
  "author": "Umar Khokhar",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "csvtojson": "^1.1.9",
    "decompress": "^4.2.0",
    "express": "^4.16.2",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "mongodb": "^2.2.33",
    "osrm": "^5.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2"
  }
}
